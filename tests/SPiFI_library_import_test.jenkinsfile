/**
 * SPiFI_library_import_test.jenkinsfile
 *
 * Test whether or not we can import the SPiFI library.
 *
 * Nothing special here right now, just tries to import the SPiFI
 * library.  If there's a problem, then Jenkins should fail the
 * job.
 */

/**
 * Configure job properties
 *
 * - Keep up to 30 entries in history for up to 90 days.
 * - Run every night at 4:00 AM
 */
properties([buildDiscarder(logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '',
                                      daysToKeepStr: '90',       numToKeepStr: '30')),
            pipelineTriggers([cron('0 4 * * *')])
           ])



node()
{
    timeout(time: 90, unit: "SECONDS")
    {

        @Library('SPiFI@master') _

    }  // Timeout

}  // Node

